@model ICollection<UserDto>
@{
    ViewData["Title"] = "Subscribe";
}
<div>
    @foreach (var user in Model)
    {
        <form asp-controller="Action" asp-action="Subscribe" asp-route-id="@user.Id" method="post" asp-anti-forgery="true">
            <button type="submit">@user.Name @user.LastName</button>
        </form>
    }
</div>


